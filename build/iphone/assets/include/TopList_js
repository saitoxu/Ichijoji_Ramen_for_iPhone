(function() {
	// name space for camera
	app.topList = {};

	// tab object
	app.topList.createTab = function() {
		// create win
		var topListWin = Ti.UI.createWindow({
			title : 'リスト',
			backgroundColor : '#fff'
		});

		// create tab
		tab = Ti.UI.createTab({
			icon : './images/list.png',
			title : 'リスト',
			window : topListWin
		});

		var tableView = Ti.UI.createTableView();
		var row1 = Ti.UI.createTableViewRow({
			title : 'pika-shi'
		});
		var row2 = Ti.UI.createTableViewRow({
			title : 'saitoxu'
		});
		var row3 = Ti.UI.createTableViewRow({
			title : 'sunlight'
		});
		// row3.add(Ti.UI.createLabel({
		// title : 'sunlight'
		// }));
		tableView.appendRow(row1);
		tableView.appendRow(row2);
		tableView.appendRow(row3);

		var view = Ti.UI.createView();

		view.add(tableView);
		topListWin.add(view);

		// create button for add task
		var button = Ti.UI.createButton({
			systemButton : Ti.UI.iPhone.SystemButton.CAMERA
		});
		// click addtask button
		button.addEventListener('click', function() {
			var cameraWindow = app.camera.createWindow();
			cameraWindow.title = "カメラ";
			cameraWindow.open();
		});

		// set button
		topListWin.setRightNavButton(button);
		return tab;
	};
})();
